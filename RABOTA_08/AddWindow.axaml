<Window xmlns="https://github.com/avaloniaui"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        mc:Ignorable="d" d:DesignWidth="800" d:DesignHeight="450"
        x:Class="RABOTA_08.AddWindow"
        Title="Создание заявки">
    <Grid>
        <TextBox HorizontalAlignment="Center" Text="Оборудование" Name="Equip"></TextBox>
        <TextBox HorizontalAlignment="Center" Text="Серийный номер" Name="SerialN"></TextBox>
        <TextBox HorizontalAlignment="Center" Text="Описание проблемы" Name="Description"></TextBox>
        <TextBox HorizontalAlignment="Center" Text="Тип оборудования" Name="Type"></TextBox>
        <TextBox HorizontalAlignment="Center" Text="Приоритет" Name="Priority"></TextBox>
        <TextBox HorizontalAlignment="Center" Text="Клиент" Name="Client"></TextBox>
        <Button Name="AddBtn" Click="AddBtn_OnClick" Content="Добавить"></Button>
    </Grid>
</Window>
